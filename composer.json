{
    "name": "andriichuk/laracash",
    "description": "PHP Laravel Money Package",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Money",
        "Laracash",
        "ValueObject"
    ],
    "authors": [
        {
            "name": "Serhii Andriichuk",
            "email": "andriichuk29@gmail.com",
            "homepage": "https://github.com/andriichuk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "illuminate/contracts": "^7.2|^8.0",
        "illuminate/support": "^7.2|^8.0",
        "illuminate/config": "^7.2|^8.0",
        "moneyphp/money": "^3.3.1"
    },
    "require-dev": {
        "ext-bcmath": "*",
        "ext-gmp": "*",
        "ext-intl": "*",
        "orchestra/testbench": "5.5.0",
        "phpunit/phpunit": "8.5.8"
    },
    "autoload": {
        "psr-4": {
            "Andriichuk\\Laracash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andriichuk\\Laracash\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        },
        "laravel": {
            "providers": [
                "Andriichuk\\Laracash\\ServiceProviders\\LaracashServiceProvider"
            ],
            "aliases": {
                "Laracash": "Andriichuk\\Laracash\\Facades\\Laracash"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
